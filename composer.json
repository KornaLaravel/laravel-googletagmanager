{
    "name": "spatie/laravel-googletagmanager",
    "description": "Google Tag Manager integration for Laravel",
    "keywords": [
        "spatie",
        "laravel-googletagmanager",
        "laravel",
        "google tag manager"
    ],
    "homepage": "https://github.com/spatie/laravel-googletagmanager",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/config": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/session": "^12.0",
        "illuminate/view": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\GoogleTagManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\GoogleTagManager\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Spatie\\GoogleTagManager\\GoogleTagManagerServiceProvider"
            ],
            "aliases": {
                "GoogleTagManager": "Spatie\\GoogleTagManager\\GoogleTagManagerFacade"
            }
        }
    },
    "config": {
        "lock": false,
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^4.0"
    }
}
